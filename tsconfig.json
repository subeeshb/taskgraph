{
  "compilerOptions": {
    "target": "es2015",
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "outDir": "lib",
    "declaration": true,
    "moduleResolution": "Node"
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}
